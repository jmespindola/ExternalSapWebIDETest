<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="capu.controller.Detail">
	<Page title="Detalle" showNavButton="true" navButtonPress="onNavBack">
	<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="__form0" title="Detalle Modelo">
	    <sap.ui.layout.form:content>
	        <core:Title text="{apparelmodels>U_ModDesc}" id="__title0" emphasized="true"/>
	        <Label text="{i18n>code}" id="__label0"/>
	        <Input width="100%" id="__input0" value="{apparelmodels>Code}" editable="false" enabled="false"/>
	        <Label text="{i18n>modcode}" id="__label1"/>
	        <Input width="100%" id="__input2" value="{apparelmodels>U_ModCode}" editable="false" enabled="false"/>
	        <Label text="{i18n>moddesc}" width="100%" id="__label2"/>
	        <Input width="100%" id="__input1" value="{apparelmodels>U_ModDesc}" valueLiveUpdate="true"/>
	        <Label text="{i18n>price}" width="100%" id="__label3"/>
	        <Input width="100%" id="__input3" value="{apparelmodels>U_Price}" type="Number" valueLiveUpdate="true"/></sap.ui.layout.form:content>
	</sap.ui.layout.form:SimpleForm>
	<content>
	<IconTabHeader selectedKey="__filter1" id="__header0">
	        <items>
	            <IconTabFilter text="Colors" icon="sap-icon://task" iconColor="Critical" id="__filter1"/>
	            <IconTabFilter text="Sizes" icon="sap-icon://documents" iconColor="Positive" id="__filter2"/></items>
	    </IconTabHeader>
	</content>
	</Page>
</mvc:View>